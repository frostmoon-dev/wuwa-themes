    "tokenColors": [
        {
            "name": "Comments",
            "scope": [
                "comment",
                "punctuation.definition.comment"
            ],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#828688"
            }
        },
        {
            "name": "Keywords",
            "scope": [
                "keyword",
                "storage"
            ],
            "settings": {
                "foreground": "#80868D"
            }
        },
        {
            "name": "Types",
            "scope": [
                "storage.type",
                "support.type"
            ],
            "settings": {
                "foreground": "#60666D"
            }
        },
        {
            "name": "Language Constants",
            "scope": [
                "constant.language",
                "support.constant",
                "variable.language"
            ],
            "settings": {
                "foreground": "#D88888"
            }
        },
        {
            "name": "Variables",
            "scope": [
                "variable",
                "support.variable"
            ],
            "settings": {
                "foreground": "#1D1F20"
            }
        },
        {
            "name": "Functions",
            "scope": [
                "entity.name.function",
                "support.function"
            ],
            "settings": {
                "fontStyle": "bold",
                "foreground": "#A898C8"
            }
        },
        {
            "name": "Classes",
            "scope": [
                "entity.name.type",
                "entity.other.inherited-class",
                "support.class"
            ],
            "settings": {
                "fontStyle": "bold",
                "foreground": "#60666D"
            }
        },
        {
            "name": "Numbers",
            "scope": [
                "constant.numeric",
                "constant.character",
                "constant"
            ],
            "settings": {
                "foreground": "#de7470"
            }
        },
        {
            "name": "Strings",
            "scope": "string",
            "settings": {
                "foreground": "#787880"
            }
        },
        {
            "name": "Escape Sequences",
            "scope": "constant.character.escape",
            "settings": {
                "foreground": "#a0a6ad"
            }
        },
        {
            "name": "Punctuation",
            "scope": "punctuation",
            "settings": {
                "foreground": "#727678"
            }
        },
        {
            "name": "HTML Tag Names",
            "scope": "entity.name.tag",
            "settings": {
                "foreground": "#80868D"
            }
        },
        {
            "name": "HTML Attributes",
            "scope": [
                "meta.tag entity.other.attribute-name",
                "entity.other.attribute-name.html"
            ],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#60666D"
            }
        },
        {
            "name": "CSS Property Names",
            "scope": [
                "meta.property-name",
                "support.type.property-name"
            ],
            "settings": {
                "foreground": "#A898C8"
            }
        },
        {
            "name": "Markup Heading",
            "scope": "markup.heading",
            "settings": {
                "fontStyle": "bold",
                "foreground": "#a0a6ad"
            }
        }
    ]
}
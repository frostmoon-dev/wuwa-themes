    "tokenColors": [
        {
            "name": "Comments",
            "scope": [
                "comment",
                "punctuation.definition.comment"
            ],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#9D8A7E"
            }
        },
        {
            "name": "Keywords",
            "scope": [
                "keyword",
                "storage"
            ],
            "settings": {
                "foreground": "#C84020"
            }
        },
        {
            "name": "Types",
            "scope": [
                "storage.type",
                "support.type"
            ],
            "settings": {
                "foreground": "#7A6556"
            }
        },
        {
            "name": "Language Constants",
            "scope": [
                "constant.language",
                "support.constant",
                "variable.language"
            ],
            "settings": {
                "foreground": "#D8A860"
            }
        },
        {
            "name": "Variables",
            "scope": [
                "variable",
                "support.variable"
            ],
            "settings": {
                "foreground": "#2D2219"
            }
        },
        {
            "name": "Functions",
            "scope": [
                "entity.name.function",
                "support.function"
            ],
            "settings": {
                "fontStyle": "bold",
                "foreground": "#C89840"
            }
        },
        {
            "name": "Classes",
            "scope": [
                "entity.name.type",
                "entity.other.inherited-class",
                "support.class"
            ],
            "settings": {
                "fontStyle": "bold",
                "foreground": "#7A6556"
            }
        },
        {
            "name": "Numbers",
            "scope": [
                "constant.numeric",
                "constant.character",
                "constant"
            ],
            "settings": {
                "foreground": "#C89840"
            }
        },
        {
            "name": "Strings",
            "scope": "string",
            "settings": {
                "foreground": "#887870"
            }
        },
        {
            "name": "Escape Sequences",
            "scope": "constant.character.escape",
            "settings": {
                "foreground": "#e75130"
            }
        },
        {
            "name": "Punctuation",
            "scope": "punctuation",
            "settings": {
                "foreground": "#8D7A6E"
            }
        },
        {
            "name": "HTML Tag Names",
            "scope": "entity.name.tag",
            "settings": {
                "foreground": "#C84020"
            }
        },
        {
            "name": "HTML Attributes",
            "scope": [
                "meta.tag entity.other.attribute-name",
                "entity.other.attribute-name.html"
            ],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#7A6556"
            }
        },
        {
            "name": "CSS Property Names",
            "scope": [
                "meta.property-name",
                "support.type.property-name"
            ],
            "settings": {
                "foreground": "#C89840"
            }
        },
        {
            "name": "Markup Heading",
            "scope": "markup.heading",
            "settings": {
                "fontStyle": "bold",
                "foreground": "#e75130"
            }
        }
    ]
}
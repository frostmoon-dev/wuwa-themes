    "tokenColors": [
        {
            "name": "Comments",
            "scope": [
                "comment"
            ],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#686058"
            }
        },
        {
            "name": "Keywords",
            "scope": [
                "keyword",
                "storage"
            ],
            "settings": {
                "foreground": "#E8B878"
            }
        },
        {
            "name": "Types",
            "scope": [
                "storage.type",
                "support.type"
            ],
            "settings": {
                "foreground": "#A27769"
            }
        },
        {
            "name": "Variables",
            "scope": [
                "variable"
            ],
            "settings": {
                "foreground": "#E8E0D8"
            }
        },
        {
            "name": "Functions",
            "scope": [
                "entity.name.function"
            ],
            "settings": {
                "fontStyle": "bold",
                "foreground": "#E3B08B"
            }
        },
        {
            "name": "Classes",
            "scope": [
                "entity.name.type"
            ],
            "settings": {
                "fontStyle": "bold",
                "foreground": "#A27769"
            }
        },
        {
            "name": "Numbers",
            "scope": [
                "constant.numeric"
            ],
            "settings": {
                "foreground": "#D09060"
            }
        },
        {
            "name": "Strings",
            "scope": "string",
            "settings": {
                "foreground": "#98A080"
            }
        },
        {
            "name": "Punctuation",
            "scope": "punctuation",
            "settings": {
                "foreground": "#807870"
            }
        },
        {
            "name": "HTML Tags",
            "scope": "entity.name.tag",
            "settings": {
                "foreground": "#E8B878"
            }
        }
    ]
}
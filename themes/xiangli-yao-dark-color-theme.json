    "tokenColors": [
        {
            "name": "Comments",
            "scope": "comment",
            "settings": {
                "fontStyle": "italic",
                "foreground": "#5A5A68"
            }
        },
        {
            "name": "Keywords",
            "scope": [
                "keyword",
                "storage"
            ],
            "settings": {
                "foreground": "#6070B0"
            }
        },
        {
            "name": "Strings",
            "scope": "string",
            "settings": {
                "foreground": "#7A9090"
            }
        },
        {
            "name": "Numbers",
            "scope": "constant.numeric",
            "settings": {
                "foreground": "#878690"
            }
        },
        {
            "name": "Functions",
            "scope": "entity.name.function",
            "settings": {
                "fontStyle": "bold",
                "foreground": "#6070B0"
            }
        },
        {
            "name": "Variables",
            "scope": "variable",
            "settings": {
                "foreground": "#dbd7d5"
            }
        },
        {
            "name": "Classes",
            "scope": "entity.name.type",
            "settings": {
                "fontStyle": "bold",
                "foreground": "#7080C0"
            }
        }
    ]
}
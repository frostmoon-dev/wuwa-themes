    "tokenColors": [
        {
            "name": "Comments",
            "scope": [
                "comment",
                "punctuation.definition.comment"
            ],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#5A6578"
            }
        },
        {
            "name": "Comments: Preprocessor",
            "scope": "comment.block.preprocessor",
            "settings": {
                "fontStyle": "",
                "foreground": "#5A6578"
            }
        },
        {
            "name": "Comments: Documentation",
            "scope": [
                "comment.documentation",
                "comment.block.documentation"
            ],
            "settings": {
                "foreground": "#72869F"
            }
        },
        {
            "name": "Invalid - Illegal",
            "scope": "invalid.illegal",
            "settings": {
                "foreground": "#ffffff"
            }
        },
        {
            "name": "Operators",
            "scope": "keyword.operator",
            "settings": {
                "foreground": "#889AB0"
            }
        },
        {
            "name": "Keywords",
            "scope": [
                "keyword",
                "storage"
            ],
            "settings": {
                "foreground": "#D65D8A"
            }
        },
        {
            "name": "Types",
            "scope": [
                "storage.type",
                "support.type"
            ],
            "settings": {
                "foreground": "#8FA0C0"
            }
        },
        {
            "name": "Language Constants",
            "scope": [
                "constant.language",
                "support.constant",
                "variable.language"
            ],
            "settings": {
                "foreground": "#C06080"
            }
        },
        {
            "name": "Variables",
            "scope": [
                "variable",
                "support.variable"
            ],
            "settings": {
                "foreground": "#E4D8DC"
            }
        },
        {
            "name": "Functions",
            "scope": [
                "entity.name.function",
                "support.function"
            ],
            "settings": {
                "fontStyle": "bold",
                "foreground": "#D65D8A"
            }
        },
        {
            "name": "Classes",
            "scope": [
                "entity.name.type",
                "entity.other.inherited-class",
                "support.class"
            ],
            "settings": {
                "fontStyle": "bold",
                "foreground": "#8FA0C0"
            }
        },
        {
            "name": "Exceptions",
            "scope": "entity.name.exception",
            "settings": {
                "foreground": "#D65D8A"
            }
        },
        {
            "name": "Sections",
            "scope": "entity.name.section",
            "settings": {
                "fontStyle": "bold",
                "foreground": "#E4D8DC"
            }
        },
        {
            "name": "Numbers, Characters",
            "scope": [
                "constant.numeric",
                "constant.character",
                "constant"
            ],
            "settings": {
                "foreground": "#B09070"
            }
        },
        {
            "name": "Strings",
            "scope": "string",
            "settings": {
                "foreground": "#80A090"
            }
        },
        {
            "name": "Strings: Escape Sequences",
            "scope": "constant.character.escape",
            "settings": {
                "foreground": "#D65D8A"
            }
        },
        {
            "name": "Strings: Regular Expressions",
            "scope": "string.regexp",
            "settings": {
                "foreground": "#C06080"
            }
        },
        {
            "name": "Strings: Symbols",
            "scope": "constant.other.symbol",
            "settings": {
                "foreground": "#B09070"
            }
        },
        {
            "name": "Punctuation",
            "scope": "punctuation",
            "settings": {
                "foreground": "#72869F"
            }
        },
        {
            "name": "HTML: Doctype Declaration",
            "scope": [
                "meta.tag.sgml.doctype",
                "meta.tag.sgml.doctype string",
                "meta.tag.sgml.doctype entity.name.tag",
                "meta.tag.sgml punctuation.definition.tag.html"
            ],
            "settings": {
                "foreground": "#5A6578"
            }
        },
        {
            "name": "HTML: Tags",
            "scope": [
                "meta.tag",
                "punctuation.definition.tag.html",
                "punctuation.definition.tag.begin.html",
                "punctuation.definition.tag.end.html"
            ],
            "settings": {
                "foreground": "#72869F"
            }
        },
        {
            "name": "HTML: Tag Names",
            "scope": "entity.name.tag",
            "settings": {
                "foreground": "#D65D8A"
            }
        },
        {
            "name": "HTML: Attribute Names",
            "scope": [
                "meta.tag entity.other.attribute-name",

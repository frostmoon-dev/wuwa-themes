{
    "name": "Wuthering Waves : Phoebe (Dark)",
    "colors": {
        "editor.background": "#1E1C1D",
        "editor.foreground": "#E8E6E4",
        "list.activeSelectionBackground": "#5070C0",
        "list.activeSelectionForeground": "#E8E6E4",
        "list.hoverBackground": "#282628",
        "sideBar.background": "#181618",
        "sideBar.foreground": "#9b9eaa",
        "sideBarTitle.foreground": "#5070C0",
        "activityBar.background": "#141214",
        "activityBar.foreground": "#E8E6E4",
        "activityBarBadge.background": "#C8AB7E",
        "statusBar.background": "#5070C0",
        "statusBar.foreground": "#E8E6E4",
        "titleBar.activeBackground": "#141214",
        "titleBar.activeForeground": "#E8E6E4",
        "tab.activeBackground": "#1E1C1D",
        "tab.activeBorder": "#5070C0",
        "tab.inactiveBackground": "#181618",
        "editorLineNumber.foreground": "#4A484A",
        "editorCursor.foreground": "#C8AB7E",
        "editor.selectionBackground": "#5070C040",
        "button.background": "#5070C0",
        "button.foreground": "#E8E6E4",
        "button.secondaryBackground": "#b19b6e",
        "focusBorder": "#5070C080",
        "textLink.foreground": "#6080D0"
    },
    "tokenColors": [
        {
            "name": "Comments",
            "scope": "comment",
            "settings": {
                "fontStyle": "italic",
                "foreground": "#6A686A"
            }
        },
        {
            "name": "Keywords",
            "scope": [
                "keyword",
                "storage"
            ],
            "settings": {
                "foreground": "#6080D0"
            }
        },
        {
            "name": "Strings",
            "scope": "string",
            "settings": {
                "foreground": "#8A9A80"
            }
        },
        {
            "name": "Numbers",
            "scope": "constant.numeric",
            "settings": {
                "foreground": "#C8AB7E"
            }
        },
        {
            "name": "Functions",
            "scope": "entity.name.function",
            "settings": {
                "fontStyle": "bold",
                "foreground": "#6080D0"
            }
        },
        {
            "name": "Variables",
            "scope": "variable",
            "settings": {
                "foreground": "#E8E6E4"
            }
        },
        {
            "name": "Classes",
            "scope": "entity.name.type",
            "settings": {
                "fontStyle": "bold",
                "foreground": "#7090E0"
            }
        }
    ]
}
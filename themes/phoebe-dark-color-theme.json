    "tokenColors": [
        {
            "name": "Comments",
            "scope": [
                "comment"
            ],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#706850"
            }
        },
        {
            "name": "Keywords",
            "scope": [
                "keyword",
                "storage"
            ],
            "settings": {
                "foreground": "#E8C040"
            }
        },
        {
            "name": "Types",
            "scope": [
                "storage.type",
                "support.type"
            ],
            "settings": {
                "foreground": "#8A6090"
            }
        },
        {
            "name": "Variables",
            "scope": [
                "variable"
            ],
            "settings": {
                "foreground": "#F0E8D8"
            }
        },
        {
            "name": "Functions",
            "scope": [
                "entity.name.function"
            ],
            "settings": {
                "fontStyle": "bold",
                "foreground": "#D4A830"
            }
        },
        {
            "name": "Classes",
            "scope": [
                "entity.name.type"
            ],
            "settings": {
                "fontStyle": "bold",
                "foreground": "#8A6090"
            }
        },
        {
            "name": "Numbers",
            "scope": [
                "constant.numeric"
            ],
            "settings": {
                "foreground": "#E8C040"
            }
        },
        {
            "name": "Strings",
            "scope": "string",
            "settings": {
                "foreground": "#A0A080"
            }
        },
        {
            "name": "Punctuation",
            "scope": "punctuation",
            "settings": {
                "foreground": "#887878"
            }
        },
        {
            "name": "HTML Tags",
            "scope": "entity.name.tag",
            "settings": {
                "foreground": "#E8C040"
            }
        }
    ]
}
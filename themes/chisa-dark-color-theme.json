    "tokenColors": [
        {
            "name": "Comments",
            "scope": [
                "comment",
                "punctuation.definition.comment"
            ],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#746D68"
            }
        },
        {
            "name": "Keywords",
            "scope": [
                "keyword",
                "storage"
            ],
            "settings": {
                "foreground": "#f8d7bd"
            }
        },
        {
            "name": "Types",
            "scope": [
                "storage.type",
                "support.type"
            ],
            "settings": {
                "foreground": "#9f252c"
            }
        },
        {
            "name": "Language Constants",
            "scope": [
                "constant.language",
                "support.constant",
                "variable.language"
            ],
            "settings": {
                "foreground": "#E8C0A8"
            }
        },
        {
            "name": "Variables",
            "scope": [
                "variable",
                "support.variable"
            ],
            "settings": {
                "foreground": "#F5F0EB"
            }
        },
        {
            "name": "Functions",
            "scope": [
                "entity.name.function",
                "support.function"
            ],
            "settings": {
                "fontStyle": "bold",
                "foreground": "#C88850"
            }
        },
        {
            "name": "Classes",
            "scope": [
                "entity.name.type",
                "entity.other.inherited-class",
                "support.class"
            ],
            "settings": {
                "fontStyle": "bold",
                "foreground": "#9f252c"
            }
        },
        {
            "name": "Numbers",
            "scope": [
                "constant.numeric",
                "constant.character",
                "constant"
            ],
            "settings": {
                "foreground": "#C88850"
            }
        },
        {
            "name": "Strings",
            "scope": "string",
            "settings": {
                "foreground": "#B8A898"
            }
        },
        {
            "name": "Escape Sequences",
            "scope": "constant.character.escape",
            "settings": {
                "foreground": "#f8d7bd"
            }
        },
        {
            "name": "Punctuation",
            "scope": "punctuation",
            "settings": {
                "foreground": "#A89888"
            }
        },
        {
            "name": "HTML Tag Names",
            "scope": "entity.name.tag",
            "settings": {
                "foreground": "#f8d7bd"
            }
        },
        {
            "name": "HTML Attributes",
            "scope": [
                "meta.tag entity.other.attribute-name",
                "entity.other.attribute-name.html"
            ],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#9f252c"
            }
        },
        {
            "name": "CSS Property Names",
            "scope": [
                "meta.property-name",
                "support.type.property-name"
            ],
            "settings": {
                "foreground": "#C88850"
            }
        },
        {
            "name": "Markup Heading",
            "scope": "markup.heading",
            "settings": {
                "fontStyle": "bold",
                "foreground": "#9f252c"
            }
        }
    ]
}
{
    "name": "Wuthering Waves : Camellya (Dark)",
    "colors": {
        "editor.background": "#1A1818",
        "editor.foreground": "#E8E6E5",
        "list.activeSelectionBackground": "#843b3d",
        "list.activeSelectionForeground": "#E8E6E5",
        "list.hoverBackground": "#252222",
        "sideBar.background": "#151313",
        "sideBar.foreground": "#928988",
        "sideBarTitle.foreground": "#A84D4F",
        "activityBar.background": "#221e1e",
        "activityBar.foreground": "#E8E6E5",
        "activityBarBadge.background": "#843b3d",
        "statusBar.background": "#843b3d",
        "statusBar.foreground": "#E8E6E5",
        "titleBar.activeBackground": "#221e1e",
        "titleBar.activeForeground": "#E8E6E5",
        "tab.activeBackground": "#1A1818",
        "tab.activeBorder": "#A84D4F",
        "tab.inactiveBackground": "#151313",
        "editorLineNumber.foreground": "#4d3637",
        "editorCursor.foreground": "#A84D4F",
        "editor.selectionBackground": "#843b3d40",
        "button.background": "#843b3d",
        "button.foreground": "#E8E6E5",
        "button.secondaryBackground": "#4d3637",
        "focusBorder": "#843b3d80",
        "textLink.foreground": "#A84D4F"
    },
    "tokenColors": [
        {
            "name": "Comments",
            "scope": "comment",
            "settings": {
                "fontStyle": "italic",
                "foreground": "#5A5050"
            }
        },
        {
            "name": "Keywords",
            "scope": [
                "keyword",
                "storage"
            ],
            "settings": {
                "foreground": "#A84D4F"
            }
        },
        {
            "name": "Strings",
            "scope": "string",
            "settings": {
                "foreground": "#7A9090"
            }
        },
        {
            "name": "Numbers",
            "scope": "constant.numeric",
            "settings": {
                "foreground": "#928988"
            }
        },
        {
            "name": "Functions",
            "scope": "entity.name.function",
            "settings": {
                "fontStyle": "bold",
                "foreground": "#A84D4F"
            }
        },
        {
            "name": "Variables",
            "scope": "variable",
            "settings": {
                "foreground": "#E8E6E5"
            }
        },
        {
            "name": "Classes",
            "scope": "entity.name.type",
            "settings": {
                "fontStyle": "bold",
                "foreground": "#B85D5F"
            }
        }
    ]
}
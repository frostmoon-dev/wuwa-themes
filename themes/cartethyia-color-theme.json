	"tokenColors": [
		{
			"name": "Comments",
			"scope": "comment",
			"settings": {
				"fontStyle": "italic",
				"foreground": "#7890A8"
			}
		},
		{
			"name": "Keywords",
			"scope": [
				"keyword",
				"keyword.control",
				"keyword.operator.new",
				"keyword.other"
			],
			"settings": {
				"foreground": "#6BA8D8",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Storage Keywords",
			"scope": [
				"storage",
				"storage.modifier"
			],
			"settings": {
				"foreground": "#4888C0"
			}
		},
		{
			"name": "Types",
			"scope": [
				"storage.type",
				"support.type",
				"entity.name.type",
				"entity.other.inherited-class"
			],
			"settings": {
				"foreground": "#A878C0",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Language Constants",
			"scope": [
				"constant.language",
				"support.constant",
				"variable.language"
			],
			"settings": {
				"foreground": "#C8A858",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Variables",
			"scope": [
				"variable",
				"support.variable"
			],
			"settings": {
				"foreground": "#2C3845"
			}
		},
		{
			"name": "Variable Properties",
			"scope": [
				"variable.other.property",
				"variable.other.object.property",
				"support.variable.property"
			],
			"settings": {
				"foreground": "#384858"
			}
		},
		{
			"name": "Functions",
			"scope": [
				"entity.name.function",
				"support.function",
				"meta.function-call"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#C8A858"
			}
		},
		{
			"name": "Classes",
			"scope": [
				"entity.name.class",
				"support.class",
				"entity.name.type.class"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#6BA8D8"
			}
		},
		{
			"name": "Exceptions",
			"scope": "entity.name.exception",
			"settings": {
				"foreground": "#C86868"
			}
		},
		{
			"name": "Numbers",
			"scope": [
				"constant.numeric",
				"constant.character.numeric"
			],
			"settings": {
				"foreground": "#88C8F0"
			}
		},
		{
			"name": "Constants",
			"scope": [
				"constant",
				"variable.other.constant"
			],
			"settings": {
				"foreground": "#D8BC70"
			}
		},
		{
			"name": "Strings",
			"scope": [
				"string",
				"string.quoted"
			],
			"settings": {
				"foreground": "#5898B8"
			}
		},
		{
			"name": "String Interpolation",
			"scope": [
				"string.template",
				"punctuation.definition.template-expression"
			],
			"settings": {
				"foreground": "#4888C0"
			}
		},
		{
			"name": "Escape Sequences",
			"scope": "constant.character.escape",
			"settings": {
				"foreground": "#6BA8D8"
			}
		},
		{
			"name": "Regular Expressions",
			"scope": [
				"string.regexp",
				"constant.character.escape.backslash.regexp"
			],
			"settings": {
				"foreground": "#C8A858"
			}
		},
		{
			"name": "Operators",
			"scope": [
				"keyword.operator",
				"keyword.operator.logical",
				"keyword.operator.comparison"
			],
			"settings": {
				"foreground": "#4888C0"
			}
		},
		{
			"name": "Punctuation",
			"scope": [
				"punctuation",
				"punctuation.separator",
				"punctuation.terminator"
			],
			"settings": {
				"foreground": "#7890A8"
			}
		},
		{
			"name": "HTML: Tag Names",
			"scope": [
				"entity.name.tag",
				"meta.tag"
			],
			"settings": {
				"foreground": "#6BA8D8"
			}
		},
		{
			"name": "HTML: Attributes",
			"scope": [
				"meta.tag entity.other.attribute-name",
				"entity.other.attribute-name.html"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#C8A858"
			}
		},
		{
			"name": "CSS: Selectors",
			"scope": [
				"entity.name.tag.css",
				"entity.other.attribute-name.class.css",
				"entity.other.attribute-name.id.css"
			],
			"settings": {
				"foreground": "#6BA8D8"
			}
		},
		{
			"name": "CSS: Property Names",
			"scope": [
				"meta.property-name",
				"support.type.property-name"
			],
			"settings": {
				"foreground": "#4888C0"
			}
		},
		{
			"name": "CSS: Property Values",
			"scope": [
				"meta.property-value",
				"support.constant.property-value"
			],
			"settings": {
				"foreground": "#C8A858"
			}
		},
		{
			"name": "JSON: Keys",
			"scope": [
				"support.type.property-name.json",
				"string.json support.type.property-name.json"
			],
			"settings": {
				"foreground": "#4888C0"
			}
		},
		{
			"name": "Markdown: Heading",
			"scope": [
				"markup.heading",
				"entity.name.section.markdown"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#6BA8D8"
			}
		},
		{
			"name": "Markdown: Bold",
			"scope": "markup.bold",
			"settings": {
				"fontStyle": "bold",
				"foreground": "#4888C0"
			}
		},
		{
			"name": "Markdown: Italic",
			"scope": "markup.italic",
			"settings": {
				"fontStyle": "italic",
				"foreground": "#586878"
			}
		},
		{
			"name": "Markdown: Link",
			"scope": [
				"markup.underline.link",
				"string.other.link.title.markdown"
			],
			"settings": {
				"foreground": "#C8A858"
			}
		},
		{
			"name": "Markdown: Code",
			"scope": [
				"markup.inline.raw",
				"markup.fenced_code.block.markdown"
			],
			"settings": {
				"foreground": "#5898B8"
			}
		},
		{
			"name": "Markup: Deletion",
			"scope": "markup.deleted",
			"settings": {
				"foreground": "#C86868"
			}
		},
		{
			"name": "Markup: Insertion",
			"scope": "markup.inserted",
			"settings": {
				"foreground": "#60A880"
			}
		},
		{
			"name": "Markup: Changed",
			"scope": "markup.changed",
			"settings": {
				"foreground": "#D4A860"
			}
		}
	]

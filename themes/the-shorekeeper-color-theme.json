	"tokenColors": [
		{
			"name": "Comments",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#7A9AB8"
			}
		},
		{
			"name": "Keywords",
			"scope": [
				"keyword",
				"keyword.control",
				"keyword.operator.new",
				"keyword.other"
			],
			"settings": {
				"foreground": "#6BB6D6",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Storage Keywords",
			"scope": [
				"storage",
				"storage.modifier"
			],
			"settings": {
				"foreground": "#4A8AB8"
			}
		},
		{
			"name": "Types",
			"scope": [
				"storage.type",
				"support.type",
				"entity.name.type",
				"entity.other.inherited-class"
			],
			"settings": {
				"foreground": "#9B86BD",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Language Constants",
			"scope": [
				"constant.language",
				"support.constant",
				"variable.language"
			],
			"settings": {
				"foreground": "#B8A4D4",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Variables",
			"scope": [
				"variable",
				"support.variable"
			],
			"settings": {
				"foreground": "#2B5F8F"
			}
		},
		{
			"name": "Variable Properties",
			"scope": [
				"variable.other.property",
				"variable.other.object.property",
				"support.variable.property"
			],
			"settings": {
				"foreground": "#3A6F9F"
			}
		},
		{
			"name": "Functions",
			"scope": [
				"entity.name.function",
				"support.function",
				"meta.function-call"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#6BB6D6"
			}
		},
		{
			"name": "Classes",
			"scope": [
				"entity.name.class",
				"support.class",
				"entity.name.type.class"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#9B86BD"
			}
		},
		{
			"name": "Exceptions",
			"scope": "entity.name.exception",
			"settings": {
				"foreground": "#C85F75"
			}
		},
		{
			"name": "Numbers",
			"scope": [
				"constant.numeric",
				"constant.character.numeric"
			],
			"settings": {
				"foreground": "#89CFF0"
			}
		},
		{
			"name": "Constants",
			"scope": [
				"constant",
				"variable.other.constant"
			],
			"settings": {
				"foreground": "#B8A4D4"
			}
		},
		{
			"name": "Strings",
			"scope": [
				"string",
				"string.quoted"
			],
			"settings": {
				"foreground": "#5A9DB0"
			}
		},
		{
			"name": "String Interpolation",
			"scope": [
				"string.template",
				"punctuation.definition.template-expression"
			],
			"settings": {
				"foreground": "#4A8AB8"
			}
		},
		{
			"name": "Escape Sequences",
			"scope": "constant.character.escape",
			"settings": {
				"foreground": "#6BB6D6"
			}
		},
		{
			"name": "Regular Expressions",
			"scope": [
				"string.regexp",
				"constant.character.escape.backslash.regexp"
			],
			"settings": {
				"foreground": "#9B86BD"
			}
		},
		{
			"name": "Operators",
			"scope": [
				"keyword.operator",
				"keyword.operator.logical",
				"keyword.operator.comparison"
			],
			"settings": {
				"foreground": "#4A8AB8"
			}
		},
		{
			"name": "Punctuation",
			"scope": [
				"punctuation",
				"punctuation.separator",
				"punctuation.terminator"
			],
			"settings": {
				"foreground": "#7A9AB8"
			}
		},
		{
			"name": "HTML: Tag Names",
			"scope": [
				"entity.name.tag",
				"meta.tag"
			],
			"settings": {
				"foreground": "#6BB6D6"
			}
		},
		{
			"name": "HTML: Attributes",
			"scope": [
				"meta.tag entity.other.attribute-name",
				"entity.other.attribute-name.html"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#9B86BD"
			}
		},
		{
			"name": "CSS: Selectors",
			"scope": [
				"entity.name.tag.css",
				"entity.other.attribute-name.class.css",
				"entity.other.attribute-name.id.css"
			],
			"settings": {
				"foreground": "#6BB6D6"
			}
		},
		{
			"name": "CSS: Property Names",
			"scope": [
				"meta.property-name",
				"support.type.property-name"
			],
			"settings": {
				"foreground": "#4A8AB8"
			}
		},
		{
			"name": "CSS: Property Values",
			"scope": [
				"meta.property-value",
				"support.constant.property-value"
			],
			"settings": {
				"foreground": "#9B86BD"
			}
		},
		{
			"name": "JSON: Keys",
			"scope": [
				"support.type.property-name.json",
				"string.json support.type.property-name.json"
			],
			"settings": {
				"foreground": "#4A8AB8"
			}
		},
		{
			"name": "Markdown: Heading",
			"scope": [
				"markup.heading",
				"entity.name.section.markdown"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#6BB6D6"
			}
		},
		{
			"name": "Markdown: Bold",
			"scope": "markup.bold",
			"settings": {
				"fontStyle": "bold",
				"foreground": "#4A8AB8"
			}
		},
		{
			"name": "Markdown: Italic",
			"scope": "markup.italic",
			"settings": {
				"fontStyle": "italic",
				"foreground": "#5A8AB0"
			}

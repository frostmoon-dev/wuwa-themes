		{
			"name": "Markdown: Link",
			"scope": [
				"markup.underline.link",
				"string.other.link.title.markdown"
			],
			"settings": {
				"foreground": "#9B86BD"
			}
		},
		{
			"name": "Markdown: Code",
			"scope": [
				"markup.inline.raw",
				"markup.fenced_code.block.markdown"
			],
			"settings": {
				"foreground": "#5A9DB0"
			}
		},
		{
			"name": "Markup: Deletion",
			"scope": "markup.deleted",
			"settings": {
				"foreground": "#C85F75"
			}
		},
		{
			"name": "Markup: Insertion",
			"scope": "markup.inserted",
			"settings": {

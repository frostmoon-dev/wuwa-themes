    "tokenColors": [
        {
            "name": "Comments",
            "scope": [
                "comment"
            ],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#507880"
            }
        },
        {
            "name": "Keywords",
            "scope": [
                "keyword",
                "storage"
            ],
            "settings": {
                "foreground": "#40B8C0"
            }
        },
        {
            "name": "Types",
            "scope": [
                "storage.type",
                "support.type"
            ],
            "settings": {
                "foreground": "#D09030"
            }
        },
        {
            "name": "Variables",
            "scope": [
                "variable"
            ],
            "settings": {
                "foreground": "#E0F0F0"
            }
        },
        {
            "name": "Functions",
            "scope": [
                "entity.name.function"
            ],
            "settings": {
                "fontStyle": "bold",
                "foreground": "#40B8C0"
            }
        },
        {
            "name": "Classes",
            "scope": [
                "entity.name.type"
            ],
            "settings": {
                "fontStyle": "bold",
                "foreground": "#D09030"
            }
        },
        {
            "name": "Numbers",
            "scope": [
                "constant.numeric"
            ],
            "settings": {
                "foreground": "#D09030"
            }
        },
        {
            "name": "Strings",
            "scope": "string",
            "settings": {
                "foreground": "#80A0A0"
            }
        },
        {
            "name": "Punctuation",
            "scope": "punctuation",
            "settings": {
                "foreground": "#6A8890"
            }
        },
        {
            "name": "HTML Tags",
            "scope": "entity.name.tag",
            "settings": {
                "foreground": "#40B8C0"
            }
        }
    ]
}
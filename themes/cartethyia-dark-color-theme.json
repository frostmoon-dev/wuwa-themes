{
    "name": "Wuthering Waves : Cartethyia (Dark)",
    "colors": {
        "editor.background": "#2A2628",
        "editor.foreground": "#E8E6E4",
        "list.activeSelectionBackground": "#4A6DC0",
        "list.activeSelectionForeground": "#E8E6E4",
        "list.hoverBackground": "#322E30",
        "sideBar.background": "#1E1C1E",
        "sideBar.foreground": "#aaafc0",
        "sideBarTitle.foreground": "#5A7DD0",
        "activityBar.background": "#181618",
        "activityBar.foreground": "#E8E6E4",
        "activityBarBadge.background": "#E2BEA9",
        "statusBar.background": "#4A6DC0",
        "statusBar.foreground": "#E8E6E4",
        "titleBar.activeBackground": "#181618",
        "titleBar.activeForeground": "#aaafc0",
        "tab.activeBackground": "#2A2628",
        "tab.activeBorder": "#5A7DD0",
        "tab.inactiveBackground": "#1E1C1E",
        "editorLineNumber.foreground": "#5A5660",
        "editorCursor.foreground": "#E2BEA9",
        "editor.selectionBackground": "#4A6DC040",
        "button.background": "#4A6DC0",
        "button.foreground": "#E8E6E4",
        "button.secondaryBackground": "#d2ae99",
        "focusBorder": "#4A6DC080",
        "textLink.foreground": "#6A8DE0"
    },
    "tokenColors": [
        {
            "name": "Comments",
            "scope": "comment",
            "settings": {
                "fontStyle": "italic",
                "foreground": "#8A8A98"
            }
        },
        {
            "name": "Keywords",
            "scope": [
                "keyword",
                "storage"
            ],
            "settings": {
                "foreground": "#6A8DE0"
            }
        },
        {
            "name": "Strings",
            "scope": "string",
            "settings": {
                "foreground": "#8AA080"
            }
        },
        {
            "name": "Numbers",
            "scope": "constant.numeric",
            "settings": {
                "foreground": "#E2BEA9"
            }
        },
        {
            "name": "Functions",
            "scope": "entity.name.function",
            "settings": {
                "fontStyle": "bold",
                "foreground": "#6A8DE0"
            }
        },
        {
            "name": "Variables",
            "scope": "variable",
            "settings": {
                "foreground": "#E8E6E4"
            }
        },
        {
            "name": "Classes",
            "scope": "entity.name.type",
            "settings": {
                "fontStyle": "bold",
                "foreground": "#7A9DF0"
            }
        }
    ]
}
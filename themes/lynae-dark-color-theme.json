    "tokenColors": [
        {
            "name": "Comments",
            "scope": [
                "comment",
                "punctuation.definition.comment"
            ],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#6A7280"
            }
        },
        {
            "name": "Keywords",
            "scope": [
                "keyword",
                "storage"
            ],
            "settings": {
                "foreground": "#B887A5"
            }
        },
        {
            "name": "Strings",
            "scope": "string",
            "settings": {
                "foreground": "#79B2B9"
            }
        },
        {
            "name": "Numbers",
            "scope": "constant.numeric",
            "settings": {
                "foreground": "#D4BCB5"
            }
        },
        {
            "name": "Types",
            "scope": [
                "storage.type",
                "support.type"
            ],
            "settings": {
                "foreground": "#6A9AAA"
            }
        },
        {
            "name": "Functions",
            "scope": [
                "entity.name.function",
                "support.function"
            ],
            "settings": {
                "fontStyle": "bold",
                "foreground": "#79B2B9"
            }
        },
        {
            "name": "Variables",
            "scope": [
                "variable",
                "support.variable"
            ],
            "settings": {
                "foreground": "#E0DCD8"
            }
        },
        {
            "name": "Classes",
            "scope": [
                "entity.name.type",
                "entity.other.inherited-class",
                "support.class"
            ],
            "settings": {
                "fontStyle": "bold",
                "foreground": "#C898B0"
            }
        },
        {
            "name": "Constants",
            "scope": "constant.language",
            "settings": {
                "foreground": "#D4BCB5"
            }
        },
        {
            "name": "Operators",
            "scope": "keyword.operator",
            "settings": {
                "foreground": "#A09890"
            }
        },
        {
            "name": "Punctuation",
            "scope": "punctuation",
            "settings": {
                "foreground": "#A09890"
            }
        },
        {
            "name": "HTML Tags",
            "scope": [
                "entity.name.tag",
                "punctuation.definition.tag"
            ],
            "settings": {
                "foreground": "#79B2B9"
            }
        },
        {
            "name": "HTML Attributes",
            "scope": "entity.other.attribute-name",
            "settings": {
                "foreground": "#B887A5"
            }
        },
        {
            "name": "CSS Properties",
            "scope": "support.type.property-name.css",
            "settings": {
                "foreground": "#6A9AAA"
            }
        },
        {
            "name": "CSS Values",
            "scope": "support.constant.property-value.css",
            "settings": {
                "foreground": "#79B2B9"
            }
        },
        {
            "name": "JSON Keys",
            "scope": "support.type.property-name.json",
            "settings": {
                "foreground": "#B887A5"
            }
        },
        {
            "name": "JSON Values",
            "scope": "string.quoted.double.json",
            "settings": {
                "foreground": "#79B2B9"
            }
        },
        {
            "name": "Markdown Headings",
            "scope": [
                "markup.heading",
                "entity.name.section"
            ],
            "settings": {
                "fontStyle": "bold",
                "foreground": "#C898B0"
            }
        },
        {
            "name": "Markdown Bold",
            "scope": "markup.bold",
            "settings": {
                "fontStyle": "bold",
                "foreground": "#E0DCD8"
            }
        },
        {
            "name": "Markdown Italic",
            "scope": "markup.italic",
            "settings": {
                "fontStyle": "italic",
                "foreground": "#D4BCB5"
            }
        },
        {
            "name": "Markdown Links",
            "scope": "markup.underline.link",
            "settings": {
                "foreground": "#79B2B9"
            }
        },
        {
            "name": "Invalid - Illegal",
            "scope": "invalid.illegal",
            "settings": {
                "foreground": "#D87A8A"
            }
        }
    ]
}
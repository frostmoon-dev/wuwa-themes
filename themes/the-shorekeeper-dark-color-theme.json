	"tokenColors": [
		{
			"name": "Comments",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#5A7A90"
			}
		},
		{
			"name": "Keywords",
			"scope": [
				"keyword",
				"keyword.control",
				"keyword.operator.new",
				"keyword.other"
			],
			"settings": {
				"foreground": "#89CFF0",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Storage Keywords",

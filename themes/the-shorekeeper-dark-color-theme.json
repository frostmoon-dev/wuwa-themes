    "tokenColors": [
        {
            "name": "Comments",
            "scope": [
                "comment"
            ],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#406878"
            }
        },
        {
            "name": "Keywords",
            "scope": [
                "keyword",
                "storage"
            ],
            "settings": {
                "foreground": "#40D8F0"
            }
        },
        {
            "name": "Types",
            "scope": [
                "storage.type",
                "support.type"
            ],
            "settings": {
                "foreground": "#8080C0"
            }
        },
        {
            "name": "Variables",
            "scope": [
                "variable"
            ],
            "settings": {
                "foreground": "#E0F0F8"
            }
        },
        {
            "name": "Functions",
            "scope": [
                "entity.name.function"
            ],
            "settings": {
                "fontStyle": "bold",
                "foreground": "#00C0E0"
            }
        },
        {
            "name": "Classes",
            "scope": [
                "entity.name.type"
            ],
            "settings": {
                "fontStyle": "bold",
                "foreground": "#8080C0"
            }
        },
        {
            "name": "Numbers",
            "scope": [
                "constant.numeric"
            ],
            "settings": {
                "foreground": "#60B0D0"
            }
        },
        {
            "name": "Strings",
            "scope": "string",
            "settings": {
                "foreground": "#80A0B0"
            }
        },
        {
            "name": "Punctuation",
            "scope": "punctuation",
            "settings": {
                "foreground": "#608090"
            }
        },
        {
            "name": "HTML Tags",
            "scope": "entity.name.tag",
            "settings": {
                "foreground": "#40D8F0"
            }
        }
    ]
}